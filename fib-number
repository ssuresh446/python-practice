n=int(input('enter the value upto which you want to find fib : '))
def fib(n):
    a=0
    b=1
    if n<=1 :
        print(a)
    elif n>1:
        print(a)
        print(b)
        for i in range(2,n):
            c=a+b
            #print(c)
            a=b
            b=c
            print(c)
    elif n<0:
        print('you have given invalid number')

fib(n)

