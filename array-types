from numpy import *
a1=array([1,2,3,4,5,6,7],float)
print(a1)
a2=array([1.2,3.4,5.6,7.8],int)
print(a2)
a3=array(['s','u','r','e','s','h'],)
print(a3[5])

l=linspace(1,15,16)
print(l)

l1=linspace(1,15)
print(l1)

arr=arange(1,15,4)
print(arr)

z=zeros(5)
print(z)

o=ones(5)
print(o)

lo=logspace(1,40,5)
print(lo)


