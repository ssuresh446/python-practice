from time import sleep
from threading import *
class hello(Thread):
    def run(self):
        for i in range(10):
            print('hello',i)
            sleep(1)
class hi(Thread):
    def run(self):
        for j in range(10):
            print('hi',j)
            sleep(1)

t1=hello()
t2=hi()
t1.start()
t2.start()
output:
