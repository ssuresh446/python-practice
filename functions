def suresh():
    print()
    x=input('enter what do you want : ')
    if x=='name':
        print('suresh')
    elif x=='number':
        print('8790923747')
    elif x=='dob':
        print('09051990')
    elif x=='village':
        print('Harischandrapuram')
    elif x=='father':
        print('amaraiah')
    elif x=='mother':
        print('nagapadma')
    elif x=='bro1':
        print('scsr')
    elif x=='bro2':
        print('sai')
    suresh()
suresh()

